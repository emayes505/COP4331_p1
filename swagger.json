                "schema": {
                  "$ref": "#/components/schemas/LoginResponse"
                },
                "example": {
                  "success": true,
                  "id": 18,
                  "firstName": "Tony",
                  "lastName": "Stark",
                  "error": ""
                }
              }
            }
          }
        }
      }
    },
